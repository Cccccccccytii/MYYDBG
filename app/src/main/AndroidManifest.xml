    <uses-permission android:name="android.permission.READ_CONTACTS"/>

    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>


    <application android:theme="@style/AppTheme"
        android:supportsRtl="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:allowBackup="true">
        <activity android:name="com.example.thinkpad.sousuokuang2.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.thinkpad.sousuokuang2.SecondActivity"></activity>
        <activity android:name="com.example.thinkpad.sousuokuang2.NewContact"></activity>

        <activity android:name="com.example.thinkpad.sousuokuang2.ChangeActivity"></activity>

</application>

</manifest>